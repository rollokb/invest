{
  "host_env_var_prefix": "INVEST",
  "file_path": "./docker/.env",
  "env_vars": {
    "required": [
      "PORT",
      "SECRET_KEY",
      "RECAPTCHA_PUBLIC_KEY",
      "RECAPTCHA_PRIVATE_KEY",
      "NOCAPTCHA",
      "ZENDESK_EMAIL",
      "ZENDESK_SUBDOMAIN",
      "ZENDESK_TOKEN"
    ],
    "optional": [
        "SECURE_SSL_REDIRECT"
    ]
  }
}
