{
  "host_env_var_prefix": "INVEST",
  "file_path": "./docker/.env",
  "env_vars": {
    "required": [
      "PORT",
      "SECRET_KEY",
      "REDIS_URL"
    ],
    "optional": [
        "ALLOWED_HOSTS",
        "SECURE_SSL_REDIRECT",
        "AWS_STORAGE_BUCKET_NAME",
        "AWS_ACCESS_KEY_ID",
        "AWS_SECRET_ACCESS_KEY",
        "AWS_S3_CUSTOM_DOMAIN"
    ]
  }
}
