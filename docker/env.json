{
  "host_env_var_prefix": "INVEST",
  "file_path": "./docker/.env",
  "env_vars": {
    "required": [
      "PORT",
      "SECRET_KEY"
    ],
    "optional": [
        "SECURE_SSL_REDIRECT",
        "DEBUG",
        "ENABLE_DEBUG_TOOLBAR",
        "SENTRY_DSN",
        "AWS_STORAGE_BUCKET_NAME",
        "AWS_ACCESS_KEY_ID",
        "AWS_SECRET_ACCESS_KEY"
    ]
  }
}
